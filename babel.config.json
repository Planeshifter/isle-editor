{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [ "@babel/plugin-transform-runtime", {
      "regenerator": true
    }],
    "@babel/plugin-transform-react-jsx",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-syntax-dynamic-import",
    "transform-modern-regexp"
  ],
  "env": {
    "development": {
      "presets": []
    },
    "test": {
      "plugins": [
        [
          "module-resolver", {
            "root": [
                "app",
                "packages"
              ]
            }
        ]
      ]
    }
  }
}
