  "main": "main.js",
  "bin": {
    "isle-bundler": "./app/bundler/bin/cli.js",
    "isle-editor": "./bin/cli.js"
  },
  "scripts": {
    "analyze": "source-map-explorer dist/renderer.prod.js dist/renderer.prod.js.map",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "cross-env NODE_ENV=production node --max_old_space_size=16384 -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.electron.js --progress --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=16384 -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.production.js --progress --colors",
    "clean": "rm -rf node_modules",
    "coverage": "jest && codecov",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "init-docs": "gitbook install",
    "lint": "eslint app test *.js && stylelint \"app/**/*.css\"",
    "package": "cross-env NODE_ENV=production node --max-old-space-size=16384 -r @babel/register package.js",
    "package-all": "npm run package -- --all",
    "publish-docs": "gitbook build && echo isledocs.com > _book/CNAME && gh-pages --dist _book",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./main.development.js",
    "start-renderer-dev": "cross-env START_HOT=1 NODE_ENV=development node --max-old-space-size=16384 --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config=webpack.config.development.js --mode=development",
    "test": "jest",
    "update-docs": "cross-env NODE_PATH=./node_modules/@stdlib/stdlib/lib/node_modules node ./scripts/update_docs.js",
    "view-docs": "concurrently --kill-others \"gitbook serve\" \"sensible-browser http://localhost:4000\""
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/file_mock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/style_mock.js"
    },
    "testURL": "http://localhost/"
  },
