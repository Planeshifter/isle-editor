{
  "name": "isle-editor",
  "productName": "ISLE Editor",
  "version": "0.19.1",
  "description": "Editor for Integrated Statistics Learning Environment (ISLE) lessons.",
  "main": "main.js",
  "bin": {
    "isle-editor": "./bin/cli.js",
    "isle-bundler": "./app/bundler/bin/cli.js"
  },
  "scripts": {
    "analyze": "source-map-explorer dist/renderer.prod.js dist/renderer.prod.js.map",
    "clean": "rm -rf node_modules",
    "update-docs": "cross-env NODE_PATH=./node_modules/@stdlib/stdlib/lib/node_modules node ./scripts/update_docs.js",
    "test": "jest",
    "coverage": "jest && codecov",
    "lint": "eslint app test *.js",
    "build-main": "cross-env NODE_ENV=production node --max_old_space_size=4096 -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.electron.js --progress --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=4096 -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.production.js --progress --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "package": "cross-env NODE_ENV=production node --max-old-space-size=4096 -r @babel/register package.js",
    "package-all": "npm run package -- --all",
    "init-docs": "gitbook install",
    "publish-docs": "gitbook build && echo isledocs.com > _book/CNAME && gh-pages --dist _book",
    "view-docs": "concurrently --kill-others \"gitbook serve\" \"sensible-browser http://localhost:4000\"",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "start-renderer-dev": "cross-env START_HOT=1 NODE_ENV=development node --max-old-space-size=4096 --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config=webpack.config.development.js --mode=development",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./main.development.js"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/file_mock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/style_mock.js"
    }
  },
